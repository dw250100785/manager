<?xml version='1.0' encoding='UTF-8'?>
<openerp>
    <!--noupdate=“1” 表示，模块升级时候不更新本文件数据。-->
    <data noupdate="0">

        <!-- 创建一个分类-->
        <record id="module_category_born_manager_category_sale" model="ir.module.category">
            <field name="name">移动办公-销售</field>
            <field name="description">移动办公</field>
            <field name="sequence">51</field>
        </record>

        <!-- 创建一个分类-->
        <record id="module_category_born_manager_category_support" model="ir.module.category">
            <field name="name">移动办公-技术</field>
            <field name="description">移动办公</field>
            <field name="sequence">5２</field>
        </record>


        <!--定义移动办公的组-->
        <record id="group_born_manager_saler" model="res.groups">
            <field name="name">销售人员</field>
            <field name="category_id" ref="module_category_born_manager_category_sale"/>
            <field name="comment">Saler</field>
        </record>


        <record id="group_born_manager_salermanager" model="res.groups">
            <field name="name">销售经理</field>
            <field name="category_id" ref="module_category_born_manager_category_sale"/>
            <field name="implied_ids" eval="[(4, ref('group_born_manager_saler'))]"/>
            <field name="comment">SalerManager</field>
        </record>

        <record id="group_born_manager_support" model="res.groups">
            <field name="name">技术人员</field>
            <field name="category_id" ref="module_category_born_manager_category_support"/>
            <field name="comment">Support</field>
        </record>

        <record id="group_born_manager_supportmanager" model="res.groups">
            <field name="name">技术经理</field>
            <field name="category_id" ref="module_category_born_manager_category_support"/>
            <field name="comment">SupportManager</field>
            <field name="implied_ids" eval="[(4, ref('group_born_manager_support'))]"/>
        </record>

    </data>


</openerp>